<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="160.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.AddItemWindowController">
   <children>
      <Label layoutX="27.0" layoutY="22.0" text="Name" />
      <Label layoutX="27.0" layoutY="58.0" text="Serial #" />
      <Label layoutX="27.0" layoutY="94.0" text="Value" />
      <TextField fx:id="nameBox" layoutX="74.0" layoutY="18.0" prefHeight="25.0" prefWidth="200.0" />
      <TextField fx:id="serialBox" layoutX="74.0" layoutY="54.0" prefHeight="25.0" prefWidth="200.0" />
      <TextField fx:id="valueBox" layoutX="74.0" layoutY="90.0" prefHeight="25.0" prefWidth="200.0" />
      <Button layoutX="174.0" layoutY="124.0" mnemonicParsing="false" onAction="#addButtonClicked" text="Add" />
      <Button layoutX="222.0" layoutY="124.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
      <Text fx:id="errorText" fill="RED" layoutX="29.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Error" visible="false" />
   </children>
</AnchorPane>
